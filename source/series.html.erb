---
title: Series
layout: layout
layout: page
---

<% select_html_pages.reject {|p| p.data.series.nil?}.group_by {|p| p.data.series }.each do |series, pages | %>
  <h2 id="<%= h(series) %>"><%= h(series) %></h2>
    <ul>
    <% pages.each do |page| %>
      <li><%= link_to(h(page.title), page) %></li>
    <% end %>
    </ul>
  </li>
<% end %>


